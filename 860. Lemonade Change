public boolean lemonadeChange(int[] bills) {
        int five = 0;
        int ten = 0;

        for(int amount: bills){
            if(amount==10){
                if(five>=1){
                    five--;
                }else{
                    return false;
                }
                ten++;
            }else if(amount==20){
                if(ten>=1 && five>=1){
                    ten--;
                    five--;
                }else if(five>=3){
                    five-=3;
                }else{
                    return false;
                }
            }else{
                five++;
            }
        }
        return true;
    }
